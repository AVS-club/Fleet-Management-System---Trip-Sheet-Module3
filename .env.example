# Fleet Management System Environment Variables
# Copy this file to .env and fill in your actual values

# Google Maps Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_DOCUMENT_AI_ENDPOINT=https://us-documentai.googleapis.com/v1
VITE_PROJECT_ID=your_project_id_here
VITE_LOCATION=us
VITE_RC_PROCESSOR_ID=your_rc_processor_id_here
VITE_INSURANCE_PROCESSOR_ID=your_insurance_processor_id_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database URL (for migrations)
DATABASE_URL=postgresql://user:password@host:port/database

# Anthropic AI (optional - for AI features)
ANTHROPIC_API_KEY=your_anthropic_api_key_here