{
  "name": "rc-proxy-server",
  "version": "1.0.0",
  "description": "Proxy server for RC details API to handle IP whitelisting",
  "type": "module",
  "main": "rc-proxy-server.js",
  "scripts": {
    "start": "node rc-proxy-server.js",
    "dev": "node rc-proxy-server.js",
    "pm2:start": "pm2 start rc-proxy-server.js --name rc-proxy",
    "pm2:stop": "pm2 stop rc-proxy",
    "pm2:restart": "pm2 restart rc-proxy",
    "pm2:logs": "pm2 logs rc-proxy"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
